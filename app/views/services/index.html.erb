<p id="notice"><%= notice %></p>

<h1>Services</h1>

<table>
  <thead>
    <tr>
      <th>Quote number</th>
      <th>Service type</th>
      <th>Quantity</th>
      <th>Price</th>
      <th>Service total</th>
      <th>Term start</th>
      <th>Term end</th>
      <th>Term length</th>
      <th>Annual increase</th>
      <th>Crm number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @services.each do |service| %>
      <tr>
        <td><%= service.quote_number %></td>
        <td><%= service.Service_type %></td>
        <td><%= service.quantity %></td>
        <td><%= service.price %></td>
        <td><%= service.service_total %></td>
        <td><%= service.term_start %></td>
        <td><%= service.term_end %></td>
        <td><%= service.term_length %></td>
        <td><%= service.annual_increase %></td>
        <td><%= service.CRM_Number %></td>
        <td><%= link_to 'Show', service %></td>
        <td><%= link_to 'Edit', edit_service_path(service) %></td>
        <td><%= link_to 'Destroy', service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service', new_service_path %>
