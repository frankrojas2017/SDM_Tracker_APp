<p id="notice"><%= notice %></p>

<h1>Agreement Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agreement_types.each do |agreement_type| %>
      <tr>
        <td><%= agreement_type.name %></td>
        <td><%= link_to 'Show', agreement_type %></td>
        <td><%= link_to 'Edit', edit_agreement_type_path(agreement_type) %></td>
        <td><%= link_to 'Destroy', agreement_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agreement Type', new_agreement_type_path %>
